{
    "version": 2,
    "name": "Full Stack",
    "build": {
      "env": {
        "CUSTOM_ENV_VAR": "value"
      }
    },
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }
  